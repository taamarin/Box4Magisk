{
  "log": {
    "disabled": true,
    "level": "panic",
    "output": "../run/sing-box.log",
    "timestamp": false
  },
  "dns": {
    "final": "cloudflare",
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false,
    "servers": [
      {
        "tag": "cloudflare",
        "address": "https://1.1.1.1/dns-query",
        "detour": "sing-box"
      },
      {
        "tag": "googledns",
        "address": "https://8.8.8.8/dns-query",
        "detour": "sing-box"
      },
      {
        "tag": "proxyServerNameServer",
        "address": "tcp://8.8.4.4",
        "detour": "direct"
      },
      {
        "tag": "nxdomain",
        "address": "RCode://name_error"
      }
    ],
    "rules": [
      {
        "domain_suffix": [
          "googlesyndication.com"
          ],
        "server": "cloudflare",
        "disable_cache": false
      },
      {
        "geosite": [
          "category-ads-all"
        ],
        "server": "nxdomain",
        "disable_cache": false
      },
      {
        "geosite": [
          "youtube"
        ],
        "server": "googledns",
        "disable_cache": false
      }
    ]
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 9898,
      "network": [
        "udp",
        "tcp"
      ],
      "tcp_fast_open": true,
      "sniff": true,
      "sniff_override_destination": true,
      "sniff_timeout": "300ms",
      "domain_strategy": "prefer_ipv4",
      "udp_timeout": 300
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct-out"
    },
    {
      "type": "block",
      "tag": "block-out"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "tag": "sing-box",
      "type": "selector",
      "outbounds": [
        "direct",
        "sub-box"
      ]
    }
  ],
  "route": {
    "final": "sing-box",
    "geoip": {
      "download_url": "https://github.com/SagerNet/sing-geoip/releases/latest/download/geoip.db",
      "download_detour": "sub-box"
    },
    "geosite": {
      "download_url": "https://github.com/CHIZI-0618/v2ray-rules-dat/raw/release/geosite.db",
      "download_detour": "sub-box"
    },
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "network": "tcp",
        "port_range": ["5228:5230"],
        "outbound": "direct-out"
      }
    ],
    "find_process": false,
    "auto_detect_interface": false,
    "override_android_vpn": false
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "../dashboard/dist",
      "secret": "",
      "store_selected": false
    }
  }
}